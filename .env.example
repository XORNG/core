# XORNG Core Configuration

# Server Configuration
XORNG_HOST=0.0.0.0
XORNG_PORT=3000

# Redis Configuration (for memory system)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Logging
LOG_LEVEL=info

# Token Tracking
ENABLE_TOKEN_TRACKING=true

# Sub-agent Configuration
SUBAGENT_TIMEOUT_MS=30000
MAX_CONCURRENT_SUBAGENTS=10

# Docker Configuration (for isolated sub-agents)
DOCKER_NETWORK=xorng-network
